{
	"resource_group": {
		"name": "WUS_LAB_1",
		"location": "northeurope"
	},
	"virtual_network": {
		"name": "VNet",
		"address_prefix": "10.0.0.0/16"
	},
	"public_ip": {
		"name": "GatewayIP",
		"version": "IPv4"
	},
	"security_groups": [
		{
			"name": "Group1",
			"rules": [
				{
					"name": "Rule1",
					"priority": 100,
					"access": "Allow",
					"protocol": "Tcp",
					"source": {
						"address_prefixes": "'*'",
						"port_ranges": 80
					},
					"destination": {
						"address_prefixes": "'*'",
						"port_ranges": 80
					}
				}
			]
		}
	],
	"virtual_machines": [
		{
			"name": "VirtualMachineOne",
			"image": "Ubuntu2204",
			"network_security_group_name": "Group1",
			"ip_address": {
				"public": "GatewayIP",
				"private": "10.0.0.4"
			},
			"size": "Standard_B1s"
		}
	],
	"commands": [
		{
			"vm_name": "VirtualMachineOne",
			"script_file": "../somefile.sh",
			"params": "'value1' 'value2'"
		}
	]
}
